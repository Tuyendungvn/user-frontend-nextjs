{"ast":null,"code":"import { call, put } from \"redux-saga/effects\";\nimport * as services from \"@services/user\";\nimport { setLoading, actionSuccess } from \"@redux/actions/common\";\nimport { showNotification } from \"@components/ToastNotification\";\nexport function* updateUserProfileSaga(payload) {\n  const variables = payload.payload;\n  yield put(setLoading(true));\n  const response = yield call(services.updateUserProfile, variables);\n  yield put(setLoading(false));\n  const {\n    updateUserProfile: result\n  } = (response === null || response === void 0 ? void 0 : response.data) || {};\n\n  if (result) {\n    showNotification({\n      type: \"success\",\n      title: \"Thông báo\",\n      message: \"Cập nhật hồ sơ thành công!\"\n    });\n    yield put(actionSuccess());\n  } else {\n    showNotification({\n      type: \"error\",\n      title: \"Thông báo\",\n      message: \"Cập nhật hồ sơ thất bại!\"\n    });\n  }\n}","map":null,"metadata":{},"sourceType":"module"}